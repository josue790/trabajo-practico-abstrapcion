from abc import ABC, abstractmethod
class Pago(ABC):
    @abstractmethod
    def procesar_pago(self, monto):
        pass
class TarjetaCredito(Pago):
    def procesar_pago(self, monto):
        return "Pago con tarjeta de " + str(monto)
class PayPal(Pago):
    def procesar_pago(self, monto):
        return "Pago con PayPal de " + str(monto)
if __name__ == "__main__":
    # Pagos
    pago1 = TarjetaCredito()
    pago2 = PayPal()
    print("Resultado Pago Tarjeta:", pago1.procesar_pago(150))   # Pago con tarjeta de 150
    print("Resultado Pago PayPal:", pago2.procesar_pago(200))
