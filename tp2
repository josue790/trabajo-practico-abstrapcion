from abc import ABC, abstractmethod
@abstactmethod
class Pago(ABC):
    def procesar_pago(self, monto):
        pass
class TarjetaCredito(Pago):
    def procesar_pago(self, monto):
        return "Procesando pago de monto con tarjeta de crédito."
class PayPal(Pago):
    def procesar_pago(self, monto):
        return "Procesando pago de monto a través de PayPal."

